CREATE TABLE "SCOTT"."TIMELINES" 
   (	USER_ID NUMBER, 
	NAME VARCHAR2(30 BYTE), 
	ACTUAL_YEAR NUMBER, 
	RATE NUMBER, 
	DESCRIPTION VARCHAR2(400 BYTE), 
	TYPE VARCHAR2(15 BYTE), 
	LOCATION VARCHAR2(40 BYTE), 
	HAP_POINTS NUMBER, 
	SAD_POINTS NUMBER, 
	PROUD_POINT NUMBER, 
	WISH_YEAR NUMBER, 
	MOMENT_INDEX NUMBER, 
	CALC_HAPP NUMBER, 
	 PRIMARY KEY ("USER_ID", "NAME")
   )
